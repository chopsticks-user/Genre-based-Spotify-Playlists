{
  "expo": {
    "name": "playtify",
    "slug": "playtify",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.jpg",
    "scheme": "playtify",
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.frost.playtify"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/playtifylogosolo.jpg",
        "backgroundColor": "#000000"
      },
      "package": "com.frost.playtify"
    },
    "extra": {
      "eas": {
        "projectId": "1e12708a-4ceb-4995-9d6f-092037d21824"
      }
    },
    "splash": {
      "image": "./assets/images/splash.jpg",
      "backgroundColor": "#001229"
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}